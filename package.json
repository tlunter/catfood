    "sequelize-cli": "^1.7.0",
    "sqlite3": "^3.0.8"
  },
  "devDependencies": {
    "axios": "^0.5.4",
    "babelify": "^6.4.0",
    "browserify": "^11.2.0",
    "classnames": "^1.2.0",
    "history": "^1.12.5",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "react-router": "1.0.0-rc3",
    "reqwest": "^2.0.4",
    "underscore": "^1.8.3",
